<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="weaselly.gtec.com.weaselly">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/weaselly_icon_dark"
        android:label="@string/app_name"
        android:roundIcon="@drawable/weaselly_icon_dark"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TestActivity"></activity>

        <service
            android:name=".UpdateWeakService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".EndInputActivity" />
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".InputAnswerActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".AddSimpleInfoActivity" />
        <activity android:name=".InputDataActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TrainingActivity" />
    </application>

</manifest>